    <axis xyz="0 0 1"/>
    <dynamics damping="5.0" friction="1.5"/>
    <limit effort="120" lower="-2.8973" upper="2.8973" velocity="2.3925"/>
  </joint>
  <transmission name="robot_trans1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="robot_joint_1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="robot_motor1">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  <joint name="robot_joint_2" type="revolute">
    <parent link="robot_link_1"/>
    <child link="robot_link_2"/>
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-1.72" soft_upper_limit="1.72"/>
