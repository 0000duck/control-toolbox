timeHorizon 5.0

initType    0 ; 0 = zero, 1 = linear, 2 = integrated with zero control action, 3 = random

ilqg
{
	integrator RK4
    dt 0.0001
    dt_sim 0.0001
    epsilon 1e-6
	max_iterations 100
    fixedHessianCorrection true
    recordSmallestEigenvalue false
    min_cost_improvement 1e-12
    nThreads 4
    nThreadsEigen 4
    
    line_search
	{
		active true ;
		adaptive false ;
		maxIterations 10 ;
		alpha_0 1.0 ;
		n_alpha 0.5 ;
	}
}