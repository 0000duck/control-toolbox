<?xml version="1.0" ?>
<robot name="ct_example_robot" 
  xmlns:xacro="http://www.ros.org/wiki/xacro">
  <link name="world"/>
  <joint name="fix_to_world" type="fixed">
    <parent link="world"/>
    <child link="robot_link_0"/>
    <origin rpy="0.0 0.0 1.0" xyz="0.2 0.3 0.5"/>
    <!-- the offset is included on purpose for kinematics testing-->
  </joint>
  <link name="robot_link_0">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.5"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
      <mass value="2.0"/>
    </inertial>
  </link>
  <gazebo reference="robot_link_0">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
  </gazebo>
  <link name="robot_link_1">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
